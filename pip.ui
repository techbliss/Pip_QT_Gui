&lt;p style=&quot; margin-top:12px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;&quot;&gt;&lt;span style=&quot; font-family:'Courier New,courier'; font-size:16pt; font-weight:400;&quot;&gt;pip install [options] &amp;lt;requirement specifier&amp;gt; [package-index-options] ...&lt;/span&gt;&lt;/p&gt;
&lt;p style=&quot; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;&quot;&gt;&lt;span style=&quot; font-family:'Courier New,courier'; font-size:16pt; font-weight:400;&quot;&gt;pip install [options] -r &amp;lt;requirements file&amp;gt; [package-index-options] ...&lt;/span&gt;&lt;/p&gt;
